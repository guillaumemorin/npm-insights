{
  "name": "npm-insights",
  "version": "2.0.1",
  "description": "Real time analytics of package downloads",
  "bin": {
    "npm-insights:log": "dist/log/index.js"
  },
  "files": [
    "dist"
  ],
  "watch": {
    "build": "{src,test}/*.js"
  },
  "scripts": {
    "watch": "npm-watch",
    "clean": "if [ -d ./dist ]; then rm -r ./dist; fi;mkdir ./dist;",
    "build-js": "babel -d dist/ src/",
    "build": "npm run clean; npm run build-js",
    "lint": "eslint src/",
    "test": "jest",
    "coverage": "jest --coverage",
    "precommit": "pretty-quick --staged",
    "prepare": "npm run build"
  },
  "author": "Guillaume Morin",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "husky": "^2.5.0",
    "jest": "^24.8.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react-test-renderer": "^16.3.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.1",
    "apollo-link-http": "^1.5.4",
    "cli-color": "^1.2.0",
    "colorful-console-log": "^1.0.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "node-fetch": "^2.1.2",
    "print-message": "^2.1.0",
    "read-package-json": "^2.0.13",
    "systeminformation": "^5.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaumemorin/npm-insights.git"
  },
  "bugs": {
    "url": "https://github.com/guillaumemorin/npm-insights/issues"
  },
  "homepage": "https://npm-insights-app.now.sh",
  "keywords": [
    "npm",
    "insights",
    "downloads",
    "real-time",
    "analytics"
  ]
}
